{
  "name": "lispish",
  "version": "0.1.38",
  "description": "A lispish implementation in Typescript",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "iife": "dist/lispish.iife.js",
  "bin": "cli/index.js",
  "scripts": {
    "lint": "eslint \"src/**/*.ts\" \"__tests__/**/*.ts\" \"cli/**/*.js\" \"cli/**/*.js\"",
    "build": "rm -rf dist && rollup -c",
    "test": "jest --collect-coverage",
    "compile": "tsc -p ./tsconfig.json --noEmit",
    "lispish": "node ./cli/index.js",
    "build-docs": "node ./scripts/docs/buildDocs.js",
    "open-docs": "open-cli ./docs/index.html",
    "open-lcov": "open-cli ./coverage/lcov-report/index.html"
  },
  "files": [
    "dist",
    "cli/**/*"
  ],
  "jest": {
    "testResultsProcessor": "jest-junit",
    "transform": {
      ".ts": "ts-jest"
    },
    "testRegex": "/__tests__/.*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.1.1",
    "jest-junit": "^12.0.0",
    "open-cli": "^7.0.1",
    "prettier": "^2.4.0",
    "rollup": "^2.38.2",
    "rollup-plugin-gzip": "^2.5.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mojir/lispish.git"
  },
  "keywords": [
    "LISP"
  ],
  "jest-junit": {
    "outputName": "test-report.xml"
  },
  "author": "Albert Mojir",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mojir/lispish/issues"
  },
  "homepage": "https://mojir.github.io/lispish/#index"
}
