{
  "name": "lispish",
  "version": "0.0.16",
  "description": "A lispish implementation in Typescript",
  "main": "dist/lispish.js",
  "module": "dist/lispish.mjs",
  "typings": "dist/lispish.d.ts",
  "bin": "cli/index.js",
  "files": [
    "dist",
    "cli"
  ],
  "jest": {
    "testResultsProcessor": "jest-junit",
    "transform": {
      ".ts": "ts-jest"
    },
    "testRegex": "/__tests__/.*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "dependencies": {
    "colors": "^1.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.168",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.1.1",
    "jest-junit": "^12.0.0",
    "prettier": "^2.4.0",
    "rollup": "^2.38.2",
    "rollup-plugin-gzip": "^2.5.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "lint": "eslint \"src/**/*.ts\"",
    "build": "rm -rf dist && rollup -c",
    "test": "jest --collect-coverage",
    "compile": "tsc -p ./tsconfig.json --noEmit",
    "lispish": "node ./cli/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mojir/lispish.git"
  },
  "keywords": [
    "LISP"
  ],
  "jest-junit": {
    "outputName": "test-report.xml"
  },
  "author": "Albert Mojir",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mojir/lispish/issues"
  },
  "homepage": "https://github.com/mojir/lispish#readme"
}
